    <ImageView
        android:id="@+id/ovcokovokbokko"
        android:layout_width="120dp"
        android:layout_height="120dp"
        app:layout_constraintBottom_toBottomOf="@+id/splwppllpsd"
        app:layout_constraintEnd_toEndOf="@+id/dlpwlpsp"
        app:layout_constraintStart_toStartOf="@+id/dlpwlpsp"
        app:layout_constraintTop_toTopOf="@+id/splwppllpsd"
        app:srcCompat="@drawable/phoenix_it_6" />

    <com.google.android.material.card.MaterialCardView
        android:layout_width="350dp"
        app:cardCornerRadius="10dp"
        app:cardBackgroundColor="@color/black"
        android:layout_height="100dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/splwppllpsd" >

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/textView2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="You have only one attemp to choose"
                android:textSize="20sp"
                android:fontFamily="@font/phon_od_bld"
                android:textColor="@color/white"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </com.google.android.material.card.MaterialCardView>

