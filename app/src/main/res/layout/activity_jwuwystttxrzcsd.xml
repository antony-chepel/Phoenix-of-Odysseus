        android:layout_height="120dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintBottom_toBottomOf="@+id/cllcokov"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/cllcokov"
        app:srcCompat="@drawable/phoenix_it_3" />

    <ImageView
        android:id="@+id/vclplpplcvkooxkc"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginTop="50dp"
        app:layout_constraintEnd_toEndOf="@+id/sdokwkookdoksakodjixij"
        app:layout_constraintStart_toStartOf="@+id/sdokwkookdoksakodjixij"
